{
  "blocks": "always",
  "brackets": {
    "expect": "never",
    "error": true
  },
  "colons": {
    "expect": "never",
    "error": true
  },
  "colors": false,
  "commaSpace": false,
  "commentSpace": "always",
  "depthLimit": false,
  "duplicates": false,
  "efficient": "always",
  "extendPref": false,
  "globalDupe": true,
  "indentPref": 2,
  "leadingZero": "never",
  "maxErrors": false,
  "maxWarnings": false,
  "mixed": true,
  "mixins": ["test-mixin", "mixin"],
  "namingConvention": false,
  "namingConventionStrict": false,
  "none": "never",
  "noImportant": false,
  "parenSpace": false,
  "placeholders": "always",
  "prefixVarsWithDollar": "always",
  "quotePref": false,
  "semicolons": {
    "expect": "never",
    "error": true
  },
  "sortOrder": false,
  "stackedProperties": "never",
  "trailingWhitespace": false,
  "universal": false,
  "valid": false,
  "zeroUnits": "never",
  "zIndexNormalize": false
}
